{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": false,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "ESNext",
      "dom"
    ],
    "paths": {
      "visual-form": [
        "vf"
      ],
      "visual-form/*": [
        "vf/*"
      ],
      "visual-form-antd": [
        "vf-antd"
      ],
      "visual-form-antd/*": [
        "vf-antd/*"
      ]
    }
  }
}
